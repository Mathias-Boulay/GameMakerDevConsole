            <string>///Initialize
Instance_to_track = "";
Instance_string_name = ""; //Unconverted: used for checks
Instance_name = ""; //Converted

STATE = "WATCH";


//Serves as pre-allocation of memory.
for(i=0;i&lt;10;i++){
    Variables[i,0] = ""; //Serve for the name
    Variables[i,1] = ""; //Serve for the value
    //Converted values:
    VariablesName[i] = "";
    VariablesValue[i] = "";
    }

xx = device_mouse_x_to_gui(0);
yy = device_mouse_y_to_gui(0);

mx = 0;
my = 0;

hover = false;
focus = false;

box_left = 0;
box_right = 0;
